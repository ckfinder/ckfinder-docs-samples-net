        <option name="root" value="App_Data"/>
      </backend>
      <backend name="default" adapter="local" useProxyCommand="true">
        <option name="root" value="userfiles"/>
        <option name="baseUrl" value="/ckfinder/userfiles/"/>
      </backend>
    </backends>
    <resourceTypes>
